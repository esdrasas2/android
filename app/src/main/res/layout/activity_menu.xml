<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/com_facebook_share_button_text_color"
    tools:context="com.crisnello.notereader.MenuActivity"
    android:orientation="vertical"
    android:weightSum="1">

    <ImageView
        android:id="@+id/iv_filter"
        android:layout_width="match_parent"
        android:layout_height="134dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginTop="20dp"
        app:srcCompat="@drawable/datafilter" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/iv_notas"
            android:layout_width="137dp"
            android:layout_height="134dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/ic_doc"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="29dp" />

        <ImageView
            android:id="@+id/iv_sair"
            android:layout_width="137dp"
            android:layout_height="134dp"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/ic_sair" />

    </LinearLayout>

</LinearLayout>
